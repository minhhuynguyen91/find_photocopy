<div id="form_photoshop" > 
  <%= form_for PhotoShop.new, html: {class: 'ui form' } do |f|  %>
    <h4 class="ui dividing header">Photoshop informantion</h4>
    <div class="field">
      <label> Name </label>
      <%= f.text_field :name %>
    </div>

    <div class="field">
      <label> Address </label>
      <%= f.text_field :address %>
    </div>

    <div class="field">
      <label> Image Url </label>
      <%= f.text_field :image_url %>
    </div>

    <%= f.submit 'Submit', class: 'ui button' %>
  <% end %>
</div>